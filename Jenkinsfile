pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo "Pulling code from Git..."
               
            }
        }

        stage('Build') {
            steps {
                echo "Building Docker image..."
                
            }
        }

        stage('Deploy') {
            steps {
                echo "Running container..."
                
            }
        }
    }
}
